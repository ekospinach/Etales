<!doctype html>
<html lang="en" ng-app="feedback">

    <head>

        <meta charset="utf-8">

        <title>eTales feedback session</title>

        <meta name="apple-mobile-web-app-capable" content="yes" />
        <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />

        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

        <link rel="stylesheet" href="bower_components/reveal.js/css/reveal.min.css">
        <link rel="stylesheet" href="bower_components/reveal.js/css/theme/beige.css" id="theme">

        <!-- For syntax highlighting -->
        <link rel="stylesheet" href="bower_components/reveal.js/lib/css/zenburn.css">

        <style>
            .greenLetter { color  : #39b54a;}
            .orangeLetter { color : #ff983d;}
            .blueLetter { color   : #0087f0;}
            .purpleLetter { color : #8781bd;}    
            .redLetter { color    : #f26c4f;}
            .roseLetter { color   : #bd8cbf;}
            .highcharts-title{
                width: 100%;
                text-align: center;
                font-size: 20px;
            }
            .p-font{
                font-family: "League Gothic",Impact,sans-serif;
                text-shadow: none;
                text-transform: uppercase; 
                font-size: 20px;
                line-height: 20px;
            }
            .line-font p{
                font-family: "League Gothic",Impact,sans-serif;
                font-size: 20px;
                line-height: 20px;
            }
            .highcharts-subtitle{
                width: 75%;
            }
            .text-left {
                width: 33%;
                font-size: 20px;
                float: left;
                margin-bottom:0px;
                text-align: left;
            }
            .text-right {
                width: 33%;
                font-size: 20px;
                float: left;
                margin-bottom:0px;
                text-align: right;
            }
            .text-center {
                width: 33%;
                font-size: 20px;
                float: left;
                margin-bottom:0px;
                text-align: center;
            }
            table{
                font-size: 16px;
            }
            .p-center{
                text-align: center;
                margin: 0;
            }

            .normal-left{
                width: 500px;
                height: 500px;
                float: left;
            }
            .normal-sp-left{
                width: 500px;
                height: 450px;
                float: left;
            }
            .normal-sp-right{
                width: 500px;
                height: 450px;
                float: right;
            }
            .normal-right{
                width: 500px;
                height: 500px;
                float: right;
            }
            .large-small{
                width: 1024px;
                height: 250px;
            }
            .large-normal{
                width: 1024px;
                height: 500px;
            }
            .small-small-left{
                width: 500px;
                height: 300px;
                float: left;
            }
            .small-small-right{
                width: 500px;
                height: 300px;
                float: right;
            }
            .clear-both{
                clear: both;
            }
            .paddingtop-20{
                padding-top: 20px;
            }
            .padding-0{
                padding: 0;
            }
            .table-bordered .supplier1{
                background-color: #D4DCE6;
            }
            .table-bordered .supplier2{
                background-color: #D69492;
            }
            .table-bordered .supplier3{
                background-color: #FFF2CC;
            }
            .table-bordered .retailer1{
                background-color: #DFDAE4;
            }
            .table-bordered .retailer2{
                background-color: #FCD5B5;
            }
            .table-title{
                font-size: 26px;
                padding: 8px;
                text-align: center;
                margin: 0;
            }

            .table-bordered {
                border-collapse:collapse;
                font-size: 16px;
            }
            .table-bordered span{
                display: inline-block;
                height: 19px;
                width: 19px;
            }
            .table-bordered p{
                padding: 8px;
                margin: 0;
                line-height: 18px;
            }
            .table-bordered h4{
                font-size: 26px;
                font-family: "League Gothic",Impact,sans-serif;
                font-variant: 
            }
            .table-bordered > thead > tr > th,
            .table-bordered > tbody > tr > th,
            .table-bordered > tfoot > tr > th,
            .table-bordered > thead > tr > td,
            .table-bordered > tbody > tr > td,
            .table-bordered > tfoot > tr > td {
                border: 2px solid #000;
            }
            .table-bordered > thead > tr > th,
            .table-bordered > thead > tr > td {
                border-bottom-width: 2px;
            }
            .legend li {
                cursor: pointer;
                display: inline-block;
                font-size: 16px;
                line-height: 16px;
                margin: 0 10px;
            }
            li.hidden {
                color: #ddd !important;    
            }
            .circle {
                width: 16px;
                height: 16px;
                -moz-border-radius: 8px;
                -webkit-border-radius: 8px;
                border-radius: 8px;
                display: inherit;
            }
            .modal-chart {
                width: 1024px;
                height: 600px;
            }
            .fragment{
                font-size: 30px !important;
                font-weight: 600px !important;
            }
        </style>
        <!--[if lt IE 9]>
            <script src="bower_components/reveal.js/lib/js/html5shiv.js"></script>
        <![endif]-->
    </head>
    <body>
        <div class="reveal">
            <!-- Any section element inside of this container is displayed as a slide -->
            <div class="slides" ng-controller='feedBackCtrl'>
                <section>
                    <h1>{{newLabel.getContent('Feedback session')}}</h1>
                    <h3>{{newLabel.getContent('Seminar')}} : <%=  seminar %></h3>
                    <h3>{{newLabel.getContent('Period')}} : <%= period %></h3>
                    <small>{{newLabel.getContent('Designed by')}} : <a href="http://www.hcdglearning.com">{{newLabel.getContent('HCD Learning')}}</a></small>
                </section>

                <section ng-controller="negotiationResultsCtrl">
                    <section>
                        <h3><a>{{newLabel.getContent('Negotiation Results')}}</a> - {{newLabel.getContent('Volume Discounts')}}</h3>
                        <hr>
                        <highchart id="chart1" config="supplierNegotiationResults" class="normal-sp-left"></highchart>
                        <highchart id="chart2" config="retailerNegotiationResults" class="normal-sp-right"></highchart>
                        <div class="clear-both paddingtop-20"></div>
                        <h5>{{newLabel.getContent('Total Volume Discounts')}} : {{supplierNegotiationResultsTotal}} ({{newLabel.getContent('$mln')}})</h5>
                    </section>
                    <section>
                        <h3><a>{{newLabel.getContent('Negotiation Results')}}</a> - {{newLabel.getContent('Performance Bonus')}}</h3>
                        <hr>
                        <highchart id="chart3" config="supplierPerformanceBonuses" class="normal-sp-left"></highchart>
                        <highchart id="chart4" config="retailerPerformanceBonuses" class="normal-sp-right"></highchart>
                        <div class="clear-both paddingtop-20"></div>
                        <h4>{{newLabel.getContent('Total Performance Bonuses')}} : {{supplierPerformanceBonusesTotal}} ({{newLabel.getContent('$mln')}})</h4>
                    </section>
                    <section>
                        <h3><a>{{newLabel.getContent('Negotiation Results')}}</a> - {{newLabel.getContent('Other Compensation')}}</h3>
                        <hr>
                        <highchart id="chart5" config="supplierOtherCompensation" class="normal-left"></highchart>
                        <highchart id="chart6" config="retailerOtherCompensation" class="normal-right"></highchart>
                        <div class="clear-both paddingtop-20"></div>
                        <h4>{{newLabel.getContent('Total Transaction Value of Other Compensation')}} : {{supplierOtherCompensationTotal}} ({{newLabel.getContent('$mln')}})</h4>
                    </section>
                    <section>
                        <h3><a>{{newLabel.getContent('Negotiation Results')}}</a> - {{newLabel.getContent('Terms of Payment')}}</h3>
                        <hr>
                        <highchart id="chart7" config="termsofPayment" class="large-normal"></highchart>
                    </section>
                </section>

                <section ng-controller="salesCtrl">
                    <section>
                        <h3><span class="roseLetter">{{newLabel.getContent('Sales')}}</span> - {{newLabel.getContent('Elecssories')}} - {{newLabel.getContent('Previous')}}</h3>
                        <hr>
                        <highchart id="chart8" config="previousSalesVolumeElecssories" class="normal-left"></highchart>
                        <highchart id="chart9" config="previousSalesValueElecssories" class="normal-right"></highchart>
                    </section>          
                    <section>
                        <h3><span class="roseLetter">{{newLabel.getContent('Sales')}}</span> - {{newLabel.getContent('Elecssories')}} - {{newLabel.getContent('Current')}}</h3>
                        <hr>
                        <highchart id="chart10" config="currentSalesVolumeElecssories" class="normal-left"></highchart>
                        <highchart id="chart11" config="currentSalesValueElecssories" class="normal-right"></highchart>
                    </section>          
                    <section>
                        <h3><span class="roseLetter">{{newLabel.getContent('Sales')}}</span> - {{newLabel.getContent('HealthBeauties')}} - {{newLabel.getContent('Previous')}}</h3>
                        <hr>
                        <highchart id="chart12" config="previousSalesVolumeHealthBeauties" class="normal-left"></highchart>
                        <highchart id="chart13" config="previousSalesValueHealthBeauties" class="normal-right"></highchart>
                    </section>     
                    <section>
                        <h3><span class="roseLetter">{{newLabel.getContent('Sales')}}</span> - {{newLabel.getContent('HealthBeauties')}} - {{newLabel.getContent('Current')}}</h3>
                        <hr>
                        <highchart id="chart14" config="currentSalesVolumeHealthBeauties" class="normal-left"></highchart>
                        <highchart id="chart15" config="currentSalesValueHealthBeauties" class="normal-right"></highchart>
                    </section>     
                </section>

                <section ng-controller="marketSharesCtrl">
