                            </tr>
                            <tr>
                                <th><p class="p-center">{{newLabel.getContent('Weightage')}}</p></th>
                                <td><p class="p-center" style="font-size:30px;">35%</p></td>
                                <td><p class="p-center" style="font-size:30px;">35%</p></td>
                                <td><p class="p-center" style="font-size:30px;">15%</p></td>
                                <td><p class="p-center" style="font-size:30px;">15%</p></td>
                                <td></td>
                            </tr>
                            <tr ng-repeat='retailer in retailers' class="{{'retailer'+retailer.retailerID}}">
                                <td>
                                    <p class="p-center"><span style="background-color:{{retailer.color}};">&nbsp;</span>{{newLabel.getContent('Retailer')}} {{retailer.retailerID}}</p>
                                </td>
                                <td ng-repeat='d in retailer.data'>
                                    <p class="fragment p-center" ng-show='$index!=2' data-fragment-index="{{6+$index}}">{{d.value *100 | number : 1}} %</p>
