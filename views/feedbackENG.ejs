                        <highchart id="chart251" config="previousNetProfitMargins" class="normal-right"></highchart>
                    </section>
                    <section>
                        <h3><span class="purpleLetter">{{newLabel.getContent('Operating Profits')}}</span> - {{newLabel.getContent('Current')}}</h3>
