                <section ng-controller="finalScoreCtrl">
                    <h3><span class="orangeLetter">{{newLabel.getContent('Final Score')}}</h3>
                    <hr>
                    <table class="table-bordered" style="width:100%;font-size:16px;margin-bottom:20px">
                        <thead>
                            <tr>
                                <th colspan="6"><p style="font-size:26px" class="p-center">{{newLabel.getContent('Supplier Scores')}}</p></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th style="width:14%"></th>
                                <th style="width:19%"><p class="p-center">{{newLabel.getContent('Incremental market Share')}}</p></th>
                                <th style="width:19%"><p class="p-center">{{newLabel.getContent('Return on Operational Budget')}}</p></th>
                                <th style="width:19%"><p class="p-center">{{newLabel.getContent('Final Portfolio Strength')}}</p></th>
                                <th style="width:19%"><p class="p-center">{{newLabel.getContent('Final Channel Strength')}}</p></th>
                                <th style="width:10%"><p class="p-center">{{newLabel.getContent('Final Score')}}</p></th>
                            </tr>
                            <tr>
                                <th><p class="p-center">{{newLabel.getContent('Weightage')}}</p></th>
                                <td><p class="p-center" style="font-size:30px;">35%</p></td>
                                <td><p class="p-center" style="font-size:30px;">35%</p></td>
                                <td><p class="p-center" style="font-size:30px;">15%</p></td>
                                <td><p class="p-center" style="font-size:30px;">15%</p></td>
                                <td></td>
                            </tr>
                            <tr ng-repeat='supplier in suppliers' class="{{'supplier'+supplier.supplierID}}">
                                <td>
                                    <p class="p-center"><span style="background-color:{{supplier.color}};">&nbsp;</span>{{newLabel.getContent('Supplier')}} {{supplier.supplierID}}</p>
                                </td>
                                <td ng-repeat='d in supplier.data'>
                                    <p class="fragment p-center" ng-show='$index!=2' data-fragment-index="{{$index}}">{{d.value *100| number : 1}} %</p>
                                    <p class="fragment p-center" ng-show='$index==2' data-fragment-index="{{$index}}">{{d.value | number : 2}}</p>
                                </td>
                                <td>
                                    <p class="fragment p-center" data-fragment-index="{{$index*0+5}}">{{supplier.score | number : 2}}</p>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    <table class="table-bordered" style="width:100%;font-size:16px;margin-bottom:100px">
                        <thead>
                            <tr>
                                <th colspan="6"><p style="font-size:26px" class="p-center">{{newLabel.getContent('Retailer Scores')}}</p></th>
