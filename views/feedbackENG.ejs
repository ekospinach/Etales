                        <div class="chart-container" feedback-ele-rural ng-model='myModel'>
                            <div id="shareChart54" class="feedbackEleRuralChart small-small-left"></div>
                            <div id="shareChart55" class="feedbackEleRuralChart small-small-right"></div>
                            <div id="shareChart56" class="feedbackEleRuralChart small-small-left"></div>
                            <div id="shareChart57" class="feedbackEleRuralChart small-small-right"></div>
                        </div>
                        <ul id="feedBackEleRuralLegend" class="legend"></ul>
                    </section>

                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Share of Shoppers')}}</span> - {{newLabel.getContent('HealthBeauties')}} - {{newLabel.getContent('Urban Market')}}</h3>
