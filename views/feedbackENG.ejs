<!doctype html>
<html lang="en" ng-app="feedback">

  <head>
    <meta charset="utf-8">

    <title>eTales feedback session</title>

    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">

    <link rel="stylesheet" href="bower_components/reveal.js/css/reveal.min.css">
    <link rel="stylesheet" href="bower_components/reveal.js/css/theme/beige.css" id="theme">

    <!-- For syntax highlighting -->
    <link rel="stylesheet" href="bower_components/reveal.js/lib/css/zenburn.css">

    <style>
    .greenLetter { color  : #39b54a;}
    .orangeLetter { color : #ff983d;}
    .blueLetter { color   : #0087f0;}
    .purpleLetter { color : #8781bd;}    
    .redLetter { color    : #f26c4f;}
    .roseLetter { color   : #bd8cbf;}
    .highcharts-title{
      font-family: "League Gothic",Impact,sans-serif;
      text-shadow: none;
      text-transform: uppercase;
    }
    .p-font{
      font-family: "League Gothic",Impact,sans-serif;
      text-shadow: none;
      text-transform: uppercase; 
      font-size: 20px;
      line-height: 20px;
    }
    .line-font p{
      font-family: "League Gothic",Impact,sans-serif;
      font-size: 20px;
      line-height: 20px;
    }
    </style>
    <!--[if lt IE 9]>
    <script src="bower_components/reveal.js/lib/js/html5shiv.js"></script>
    <![endif]-->
  </head>

  <body>

    <div class="reveal">
      <!-- Any section element inside of this container is displayed as a slide -->
      <div class="slides">
        <section>
          <h1>Feedback session</h1>
          <h3>Seminar : <%=  seminar %></h3>
          <h3>Period : <%= period %></h3>           
          <small>Designed by <a href="http://www.hcdglobal.com">HCD Learning</a></small>            
        </section>

        <section ng-controller="negotiationResultsCtrl">
          <section>
            <h3><a>Negotiation Results</a> - Volume Discounts</h3>
            <hr>
              <highchart id="chart1" config="supplierNegotiationResults"style="width:50%;float:left;height:500px"></highchart>
              <highchart id="chart2" config="retailerNegotiationResults" style="width:50%;float:left;height:500px"></highchart>
            <p> </p>
            <h5>Total Volume Discounts : 43.4 ($mln)</h5>
          </section>
          <section>
            <h3><a>Negotiation Results</a> - Performance Bonus</h3>
            <hr>
            <highchart id="chart3" config="supplierPerformanceBonuses"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart4" config="retailerPerformanceBonuses" style="width:50%;float:left;height:500px"></highchart>
            <h4>Total Volume Discounts : 43.4 ($mln)</h4>
          </section>
          <section>
            <h3><a>Negotiation Results</a> - Other Compensation</h3>
            <highchart id="chart5" config="supplierOtherCompensation"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart6" config="retailerOtherCompensation" style="width:50%;float:left;height:500px"></highchart>
            
            <h4>Total Volume Discounts : 43.4 ($mln)</h4>
          </section>
          <section>
            <h3><a>Negotiation Results</a> - Terms of Payment</h3>
            <hr>
            <highchart id="chart7" config="termsofPayment" style="height:500px"></highchart>
            <h4>Total Volume Discounts : 43.4 ($mln)</h4>
          </section>
        </section>

        <section ng-controller="salesCtrl">
          <section>
            <h3><span class="roseLetter">Sales</span> - Elecssories - previous</h3>
            <hr>
            <highchart id="chart8" config="previousSalesVolumeElecssories"style="width:50%;float:left;height:500px"></highchart>
             <highchart id="chart9" config="previousSalesValueElecssories"style="width:50%;float:left;height:500px"></highchart>
          </section>          
          <section>
            <h3><span class="roseLetter">Sales</span> - Elecssories - current</h3>
            <hr>
            <highchart id="chart10" config="currentSalesVolumeElecssories"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart11" config="currentSalesValueElecssories"style="width:50%;float:left;height:500px"></highchart>
          </section>          
          <section>
            <h3><span class="roseLetter">Sales</span> - HealthBeauties - Previous</h3>
            <hr>
            <highchart id="chart12" config="previousSalesVolumeHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart13" config="previousSalesValueHealthBeauties"style="width:50%;float:right;height:500px"></highchart>
          </section>     
          <section>
            <h3><span class="roseLetter">Sales</span> - HealthBeauties - Current</h3>
            <hr>
            <highchart id="chart14" config="currentSalesVolumeHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart15" config="currentSalesValueHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
          </section>     
        </section>

        <section ng-controller="marketSharesCtrl">
          <section>
            <h3><span class="greenLetter">Market Shares</span> - Elecssories - Previous</h3>
            <hr>
            <highchart id="chart16" config="previousSharesVolumeElecssories"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart17" config="previousSharesValueElecssories"style="width:50%;float:left;height:500px"></highchart>
          </section>          
          <section>
            <h3><span class="greenLetter">Market Shares</span> - Elecssories - Current</h3>
            <hr>
            <highchart id="chart18" config="currentSharesVolumeElecssories"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart19" config="currentSharesValueElecssories"style="width:50%;float:left;height:500px"></highchart>
          </section>          
          <section>
            <h3><span class="greenLetter">Market Shares</span> - HealthBeauties - Previous</h3>
            <hr>
            <highchart id="chart20" config="previousSharesVolumeHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart21" config="previousSharesValueHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
          </section>     
          <section>
            <h3><span class="greenLetter">Market Shares</span> - HealthBeauties - Current</h3>
            <hr>
            <highchart id="chart22" config="currentSharesVolumeHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart23" config="currentSharesValueHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
          </section>     
        </section>

        <section ng-controller="profitsCtrl">
          <section>
            <h3><span class="purpleLetter">Profits</span> - Previous</h3>
            <hr>
            <highchart id="chart24" config="previousOperatingProfits"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart25" config="previousNetProfitMargins"style="width:50%;float:left;height:500px"></highchart>
          </section>          
          <section>
            <h3><span class="purpleLetter">Profits</span> - Current</h3>
            <hr>
            <highchart id="chart26" config="currentOperatingProfits"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart27" config="currentNetProfitMargins"style="width:50%;float:left;height:500px"></highchart>
          </section>           
        </section>

        <section ng-controller="shelfSpaceCtrl">
          <h3><span class="redLetter">Shelf Space Allocation</h3>
          <hr>
          <highchart id="chart28" config="currentShelfSpaceElecssories"style="width:50%;float:left;height:500px"></highchart>
            <highchart id="chart29" config="currentShelfSpaceHealthBeauties"style="width:50%;float:left;height:500px"></highchart>
        </section>

        <section>
          <section>
            <h3><span class="blueLetter">Supplier KPIs</span> - Effectiveness of Trade Spending</h3>
            <hr>
          </section>          
          <section>
            <h3><span class="blueLetter">Supplier KPIs</span> - Effectiveness of Marketing Spending</h3>
            <hr>
          </section>     
          <section>
            <h3><span class="blueLetter">Supplier KPIs</span> - Portfolio Strength</h3>
            <hr>
          </section>     
          <section>
            <h3><span class="blueLetter">Supplier KPIs</span> - B&M Channel Strength</h3>
            <hr>
          </section>                                           
          <section>
            <h3><span class="blueLetter">Supplier KPIs</span> - Online Channel Strength</h3>
            <hr>
          </section>                  
        </section>

        <section>
          <section>
            <h3><span class="orangeLetter">Retailer KPIs</span> - Rotation Index</h3>
            <hr>
          </section>          
          <section>
            <h3><span class="orangeLetter">Retailer KPIs</span> - Profitability Index</h3>
            <hr>
          </section>     
          <section>
            <h3><span class="orangeLetter">Retailer KPIs</span> - Profitability Index</h3>
            <hr>
          </section>     
          <section>
            <h3><span class="orangeLetter">Retailer KPIs</span> - Stock Cover</h3>
            <hr>
          </section>                                           
          <section>
            <h3><span class="orangeLetter">Retailer KPIs</span> - Share of Shoppers</h3>
            <hr>
          </section>                  
        </section>

        <section>
          <h1>THE END</h1>
          <small>Designed by <a href="http://www.hcdglobal.com">HCD Learning</a></small>
        </section>

      </div>

    </div>

    <script src="bower_components/reveal.js/lib/js/head.min.js"></script>
    <script src="bower_components/reveal.js/js/reveal.min.js"></script>
    <script src="bower_components/angular/angular.js"></script>
    <script src="bower_components/jquery/dist/jquery.js"></script>
    <script src="bower_components/highcharts/highcharts.js"></script>
    <script src="bower_components/highcharts-ng/src/highcharts-ng.js"></script>

    <script src="js/feedBackAppControllers/negotiationResultsCtrl.js"></script>
    <script src="js/feedBackAppControllers/salesCtrl.js"></script>
    <script src="js/feedBackAppControllers/marketSharesCtrl.js"></script>
    <script src="js/feedBackAppControllers/profitsCtrl.js"></script>
    <script src="js/feedBackAppControllers/shelfSpaceCtrl.js"></script>

    <script>

      // Full list of configuration options available here:
      // https://github.com/hakimel/reveal.js#configuration
      Reveal.initialize({
        controls: true,
        progress: true,
        history: true,
        center: true,

        theme: Reveal.getQueryHash().theme, // available themes are in /css/theme
        transition: Reveal.getQueryHash().transition || 'linear', // default/cube/page/concave/zoom/linear/fade/none

        // Parallax scrolling
        // parallaxBackgroundImage: 'https://s3.amazonaws.com/hakim-static/reveal-js/reveal-parallax-1.jpg',
        // parallaxBackgroundSize: '2100px 900px',

        // Optional libraries used to extend on reveal.js
        dependencies: [
          { src: 'bower_components/reveal.js/lib/js/classList.js', condition: function() { return !document.body.classList; } },
          { src: 'bower_components/reveal.js/plugin/markdown/marked.js', condition: function() { return !!document.querySelector( '[data-markdown]' ); } },
          { src: 'bower_components/reveal.js/plugin/markdown/markdown.js', condition: function() { return !!document.querySelector( '[data-markdown]' ); } },
          { src: 'bower_components/reveal.js/plugin/highlight/highlight.js', async: true, callback: function() { hljs.initHighlightingOnLoad(); } },
          { src: 'bower_components/reveal.js/plugin/zoom-js/zoom.js', async: true, condition: function() { return !!document.body.classList; } },
          { src: 'bower_components/reveal.js/plugin/notes/notes.js', async: true, condition: function() { return !!document.body.classList; } }
        ]
      });

    </script>

  </body>
</html>
