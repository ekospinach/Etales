                    <highchart id="chart28" config="currentShelfSpaceElecssories" class="normal-left"></highchart>
                    <highchart id="chart29" config="currentShelfSpaceHealthBeauties" class="normal-right"></highchart>
                </section>

                <section ng-controller="supplierKPIsCtrl">
                    <section>
                        <h3><span class="blueLetter">{{newLabel.getContent('Supplier KPIs')}}</span> - {{newLabel.getContent('Effectiveness of Trade Spending')}}</h3>
                        <hr>
                        <highchart id="chart30" config="tradeSpendingElecssories" class="normal-left"></highchart>
                        <highchart id="chart31" config="tradeSpendingHealthBeauties" class="normal-right"></highchart>
                    </section>
                    <section>
                        <h3><span class="blueLetter">{{newLabel.getContent('Supplier KPIs')}}</span> - {{newLabel.getContent('Effectiveness of Marketing Spending')}}</h3>
                        <hr>
                        <highchart id="chart32" config="marketingSpendingElecssories" class="normal-left"></highchart>
                        <highchart id="chart33" config="marketingSpendingHealthBeauties" class="normal-right"></highchart>
                    </section>     
                    <section>
                        <h3><span class="blueLetter">{{newLabel.getContent('Supplier KPIs')}}</span> - {{newLabel.getContent('Portfolio Strength')}}</h3>
                        <hr>
                        <highchart id="chart34" config="portfolioStrengthElecssories" class="normal-left"></highchart>
                        <highchart id="chart35" config="portfolioStrengthHealthBeauties" class="normal-right"></highchart>
                    </section>     
                    <section>
                        <h3><span class="blueLetter">{{newLabel.getContent('Supplier KPIs')}}</span> - {{newLabel.getContent('B&M Channel Strength')}}</h3>
                        <hr>
                        <highchart id="chart36" config="bmChannelStrengthElecssories" class="large-small"></highchart>
                        <highchart id="chart37" config="bmChannelStrengthHealthBeauties" class="large-small"></highchart>
                    </section>
                    <section>
                        <h3><span class="blueLetter">{{newLabel.getContent('Supplier KPIs')}}</span> - {{newLabel.getContent('Online Channel Strength')}}</h3>
                        <hr>
                        <highchart id="chart38" config="onlineChannelStrengthElecssories" class="large-small"></highchart>
                        <highchart id="chart39" config="onlineChannelStrengthHealthBeauties" class="large-small"></highchart>
                    </section>                  
                </section>

                <section ng-controller="retailerKPIsCtrl">
                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Retailer KPIs')}}</span> - {{newLabel.getContent('Rotation Index')}}</h3>
                        <hr>
                        <highchart id="chart40" config="rotationIndexSalesValueElecssories" class="small-small-left"></highchart>
                        <highchart id="chart41" config="rotationIndexSalesVolumeElecssories" class="small-small-right"></highchart>
                        <highchart id="chart42" config="rotationIndexSalesValueHealthBeauties" class="small-small-left"></highchart>
                        <highchart id="chart43" config="rotationIndexSalesVolumeHealthBeauties" class="small-small-right"></highchart>
                    </section>          
                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Retailer KPIs')}}</span> - {{newLabel.getContent('Profitability Index')}} - {{newLabel.getContent('Previous')}}</h3>
                        <hr>
                        <highchart id="chart45" config="previousUrbanProfitabilityIndex" class="normal-left"></highchart>
                        <highchart id="chart44" config="previousRuralProfitabilityIndex" class="normal-right"></highchart>
                    </section>     
                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Retailer KPIs')}}</span> - {{newLabel.getContent('Profitability Index')}} - {{newLabel.getContent('Current')}}</h3>
                        <hr>
                        <highchart id="chart47" config="currentUrbanProfitabilityIndex" class="normal-left"></highchart>
                        <highchart id="chart46" config="currentRuralProfitabilityIndex" class="normal-right"></highchart>
                    </section>     
                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Retailer KPIs')}}</span> - {{newLabel.getContent('Stock Cover')}}</h3>
                        <hr>
                        <highchart id="chart49" config="EStockCover" class="normal-left"></highchart>
                        <highchart id="chart48" config="HStockCover" class="normal-right"></highchart>
                        <!--<highchart id="chart49" config="urbanStockCover" class="normal-left"></highchart>
                        <highchart id="chart48" config="ruralStockCover" class="normal-right"></highchart>-->
                    </section>                                           
                </section>

                <section ng-controller="shareOfShoppersCtrl">
                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Share of Shoppers')}}</span> - {{newLabel.getContent('Elecssories')}} - {{newLabel.getContent('Urban Market')}}</h3>
                        <hr>
                        <div class="chart-container" feedback-ele-urban ng-model='myModel'>
                            <div id="shareChart50" class="feedbackEleUrbanChart small-small-left"></div>
                            <div id="shareChart51" class="feedbackEleUrbanChart small-small-right"></div>
                            <div id="shareChart52" class="feedbackEleUrbanChart small-small-left"></div>
                            <div id="shareChart53" class="feedbackEleUrbanChart small-small-right"></div>
                        </div>
                        <ul id="feedBackEleUrbanLegend" class="legend"></ul>
                    </section>
                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Share of Shoppers')}}</span> - {{newLabel.getContent('Elecssories')}} - {{newLabel.getContent('Rural Market')}}</h3>
