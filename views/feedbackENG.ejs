                        <div feedback-hea-urban ng-model='myModel'>
                            <div id="shareChart58" class="feedbackHeaUrbanChart small-small-left"></div>
                            <div id="shareChart59" class="feedbackHeaUrbanChart small-small-right"></div>
                            <div id="shareChart60" class="feedbackHeaUrbanChart small-small-left"></div>
                            <div id="shareChart61" class="feedbackHeaUrbanChart small-small-right"></div>
                        </div>
                        <ul id="feedBackHeaUrbanLegend" class="legend"></ul>
                    </section>
                    <section>
                        <h3><span class="orangeLetter">{{newLabel.getContent('Share of Shoppers')}}</span> - {{newLabel.getContent('HealthBeauties')}} - {{newLabel.getContent('Rural Market')}}</h3>
